{
    "compilerOptions": {
        "target": "es2019",
        "module": "esnext",
        "lib": [
            "DOM",
            "ESNext",
            "ES2019"
        ],
        "allowJs": true,
        "jsx": "react-jsx",
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceRoot": "src",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
